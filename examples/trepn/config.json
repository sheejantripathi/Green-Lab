{
  "type": "native",
  "devices": {
    "nokia": {
      "root_disable_charging": "True"
    }
  },
  "repetitions": 1,
  "duration": 60000,
  "apps": [
    "com.android.chrome"
  ],
  "systrace_path": "/usr/lib/android-sdk/platform-tools/systrace/systrace.py",
  "powerprofile_path": "/home/pi/android-runner/examples/batterystats/power_profile.xml",
  "monkeyrecorder_path": "/home/pi/android-runner/MonkeyPlayer/monkeyRecorder.py",
  "monkeyrunner_path": "/usr/lib/android-sdk/tools/bin/monkeyrunner",
  "randomization": true,
  "profilers": {
    "trepn": {
      "subject_aggregation": "default",
      "experiment_aggregation": "Scripts/aggregate_trepn.py",
      "preferences": {
        "profiling_interval": 100,
        "battery_power_source_selection": "Direct Power"
      },
      "data_points": [
        "battery_power","battery_remaining", "mem_usage","cpu_load","cpu1_freq","cpu_load_normalized","mobile_data","wifi"
      ]
    },
    "monkeyrecorder": {
      "subject_aggregation": "default",
      "experiment_aggregation": "Scripts/aggregate_trepn.py",
      "preferences": {
        "profiling_interval": 100,
        "battery_power_source_selection": "Direct Power"
      },
      "data_points": [
        "battery_power","battery_remaining", "mem_usage","cpu_load","cpu1_freq","cpu_load_normalized","mobile_data","wifi"
      ]
    },
    "batterystats": {
      "cleanup": "true",
      "subject_aggregation": "default",
      "experiment_aggregation": "default",
      "enable_systrace_parsing": true,
      "python2_path": "python2"
    }
  },
  "scripts": {
    "before_experiment": "Scripts/before_experiment.py",
    "before_run": "Scripts/before_run.py",
    "after_launch": "Scripts/after_launch.py",
    "interaction": [
      {
        "type": "python3",
        "path": "Scripts/interaction.py",
        "timeout": 500,
        "logcat_regex": "some keyword"
      }
    ],
    "before_close": "Scripts/before_close.py",
    "after_run": "Scripts/after_run.py",
    "after_experiment": "Scripts/after_experiment.py"
  },
  "time_between_run": 30000
}